{
  "version": "1.0.0",
  "description": "Whitelist of approved long string patterns for security scanning",
  "last_updated": "2025-01-13T11:57:00Z",
  "patterns": {
    "npm_integrity_hashes": {
      "pattern": "sha512-[A-Za-z0-9+/]{86}==?",
      "description": "NPM package integrity hashes from package-lock.json",
      "risk_level": "SAFE",
      "context": "package-lock.json"
    },
    "git_commit_hashes": {
      "pattern": "[0-9a-f]{40}",
      "description": "Git commit SHA-1 hashes",
      "risk_level": "SAFE", 
      "context": ".git/"
    },
    "uuid_v4": {
      "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}",
      "description": "UUID version 4 identifiers",
      "risk_level": "SAFE",
      "context": "application"
    },
    "base64_encoded_images": {
      "pattern": "data:image/[a-zA-Z]+;base64,[A-Za-z0-9+/]+=*",
      "description": "Base64 encoded image data URLs",
      "risk_level": "SAFE",
      "context": "frontend"
    },
    "css_content_hashes": {
      "pattern": "_next/static/css/[a-f0-9]{16}\\.css",
      "description": "Next.js CSS content hashes",
      "risk_level": "SAFE",
      "context": "build system"
    },
    "javascript_content_hashes": {
      "pattern": "_next/static/chunks/[a-f0-9-]{32,}\\.js",
      "description": "Next.js JavaScript chunk hashes", 
      "risk_level": "SAFE",
      "context": "build system"
    },
    "font_file_hashes": {
      "pattern": "UcCO3FwrK3iLTeHuS_fvQtMwCp50KnMw2boKoduKmMEVuLyfAZ9hiA",
      "description": "Google Fonts file identifier",
      "risk_level": "SAFE",
      "context": "fonts.gstatic.com"
    }
  },
  "exclusions": {
    "file_patterns": [
      "node_modules/**/*",
      ".git/**/*", 
      "*.lock",
      "*.log"
    ],
    "content_patterns": [
      "integrity.*sha512-",
      "\"integrity\":",
      "// DataTranquil Service Worker"
    ]
  },
  "scan_config": {
    "max_line_length": 500,
    "skip_binary_files": true,
    "report_context_lines": 2
  }
}
